	<item>
		<title>Nosy Fanihy</title>
		<link>http://localhost/adverts/2018/05/14/nosy-fanihy/</link>
		<pubDate>Mon, 14 May 2018 15:20:57 +0000</pubDate>
		<dc:creator><![CDATA[admin]]></dc:creator>
